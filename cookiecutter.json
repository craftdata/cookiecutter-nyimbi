{
    "project_name": "Project Name",
    "repo_name": "{{ cookiecutter.project_name.lower().replace(' ', '_') }}",
    "description": "A short description of the project.",
    "author_name": "Nyimbi Odero",
    "email": "nyimbi@gmail.com",
    "github_username": "nyimbi",
    "db_name": "{{ cookiecutter.project_name.lower().replace(' ', '_') }}",
    "company_name": "DataCraft Ltd",
    "website": "https://www.datacraft.co.ke",
    "command_line_interface": ["click", "plain", "argparse", "no"],
    "use_docker": "y",
    "version": "0.1.0.dev0",
    "timezone": "Africa/Nairobi",
    "use_celery": "n",
    "year": "{% now 'utc', '%Y' %}",
    "sphinx_theme": ["alabaster","sphinx-rtd-theme", "sphinx-py3doc-enhanced-theme"],
    "open_source_license": ["MIT", "BSD-3-Clause", "No license file"],
    "test_runner": ["pytest", "nose"],
    "linter": ["flake8", "pylama"],
    "first_app": "app_full",
    "bibtex":"yes",
    "nomenclature":"yes",
    "date": "{% now 'utc', '%d-%m-%Y / %H:%M' %}",
    "custom_bootstrap_compilation": "y",
    "_extensions": ["jinja2_time.TimeExtension"],
    "_copy_without_render": [
        "src/first_app/app/*"
    ]
}
